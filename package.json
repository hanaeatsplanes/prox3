{
	"name": "prox3",
	"version": "1.0.0",
	"type": "module",
	"main": "src/index.ts",
	"scripts": {
		"build": "bun build . --minify --outdir=dist --target=bun --footer='// made w <3 by @hanaeatsplanes'",
		"compile": "bun build . --compile --outfile=dist/app",
		"start": "bun run dist/src/index.js",
		"ci": "biome check . --error-on-warnings",
		"lint": "biome check . --fix && biome format . --write",
		"lint:unsafe": "biome check . --fix --unsafe && biome format . --write",
		"lint:check": "biome check . && biome format .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "bun run --watch src/index.ts"
	},
	"dependencies": {
		"elysia": "latest"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.4",
		"@types/bun": "latest",
		"@types/node": "latest",
		"typescript": "latest"
	}
}
